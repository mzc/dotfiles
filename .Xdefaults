
Xft.dpi:                96
Xft.antialias:          true
Xft.hinting:            full
Xft.hintstyle:          hintslight
Xft.rgba:               rgb

URxvt.background:	#2F4F4F
URxvt.transparent:      true
URxvt.inheritPixmap:    true
URxvt.imLocale:	        zh_CN.UTF-8
URxvt.inputMethod:	ibus
URxvt.scrollBar:        false
URxvt.saveLines:        32767
URxvt.cursorBlink:      true
URxvt.cursorColor:      #FFFFFF
URxvt.color:            #FFFFFF
URxvt.shading:          30
URxvt.tintcolor:        #151515
URxvt.depth:            24
URxvt.visualBall:       true

URxvt*font:             xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true,xft:WenQuanYi ZenHei Mono-10
URxvt*boldFont:         xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true:bold,xft:WenQuanYi ZenHei Mono-10:Bol
URxvt*italicFont:       xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true:italic,xft:WenQuanYi ZenHei Mono-10
URxvt*bolditalicFont:   xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true:bold:italic,xft:WenQuanYi ZenHei Mono-10

URxvt*perl-ext-common:  default,matcher,clipboard
URxvt*urlLauncher:      urxvt-to-uzbl.sh
URxvt.matcher.button:   1

